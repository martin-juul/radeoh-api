        "sass-loader": "^8.0.0",
        "vue": "~2.6.11",
        "vue-router": "~3.1.6",
        "vue-template-compiler": "~2.6.11",
        "vuetify": "~2.2.20",
        "vuex": "^3.1.3"
    },
